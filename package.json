{
  "name": "wework",
  "version": "1.0.1",
  "description": "The work wechat (wework) chat group robot webhooks api",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:clean && tsc -p tsconfig.build.json",
    "build:clean": "shx rm -rf ./dist"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.15",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:a9/wework.git"
  },
  "homepage": "https://github.com/a9/wework",
  "keywords": [
    "wechat",
    "wework",
    "bot",
    "webhooks",
    "sdk"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
